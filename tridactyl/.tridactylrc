# see default mappings in tridactyl/src/lib/config.ts

set colourscheme dark
set smoothscroll true

bind x tabclose
bind X undo
bind zx winclose

bind h back
bind l forward

bind H scrollpx -5
bind L scrollpx 5

bind d scrollline 50
bind u scrollline -50
bind e scrollline -50

bind D scrollpage 1
bind U scrollpage -1
bind E scrollpage -1

bind yt tabduplicate

bind gw tabdetach
bind gp pin
